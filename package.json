{
  "name": "lito-vscode",
  "displayName": "Lito Programming Language",
  "description": "",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "lito",
        "aliases": [
          "Lito",
          "lito"
        ],
        "extensions": [
          ".lito"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lito",
        "scopeName": "source.lito",
        "path": "./syntaxes/lito.tmLanguage.json"
      }
    ]
  },
  "publisher": "robotii",
  "homepage": "https://github.com/robotii/lito-vscode",
  "bugs": {
    "url": "https://github.com/robotii/lito-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robotii/lito-vscode.git"
  }
}
